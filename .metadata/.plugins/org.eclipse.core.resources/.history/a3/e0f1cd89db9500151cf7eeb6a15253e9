
public class Tabtables {
	final int NB_MAX = 10;
	
	Table [] tabTable;
	int nbTables;
	
	// Constructeur
	Tabtables(){
		tabTable = new Table[NB_MAX];
	}
	
	// Ajouter
	void  ajouterTable (Table uneTable) {
		tabTable[nbTables] = uneTable;
		nbTables++;
	}
	
	// afficher
	void afficherTable(){
		for(Table i : tabTable){
			System.out.println("Test de débordement" +i);
		}
	}
}
