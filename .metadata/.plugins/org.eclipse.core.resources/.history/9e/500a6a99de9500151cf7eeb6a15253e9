
public class Tabtables {
	final int NB_MAX = 10;
	
	Table [] tabTable;
	int nbTables;
	
	// Constructeur
	Tabtables(){
		tabTable = new Table[NB_MAX];
	}
	
	// Ajouter
	void  ajouterTable (Table uneTable) {
		tabTable[nbTables] = uneTable;
		nbTables++;
	}
	
	// afficher
	void afficherTable(){
		System.out.println("Contenu de la table");
		for(Table uneTable : tabTable){
			uneTable.afficher();
		}
	}
}
