
public class Table {
	int nb_pieds;
	String couleur;
	double prix;
	
	// Constructeur
	Table (){
		nb_pieds=4;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Table t1;
		t1 = new Table();
		System.out.println("Nombre de pieds : " +t1.nb_pieds);
		System.out.println("Prix : " +t1.prix);
		System.out.println("Couleur : " +t1.couleur);
		
	}

}
