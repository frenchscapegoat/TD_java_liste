import java.util.Scanner;

public class TypesDeBase {

	public static void main(String[] args) {
		//----------- 
		// Declaration de vairables locales à la méthode main():
		// Types de base qui existent en C
		//----------- 
		short s;
		int i;
		long l;
		
		float f;
		double d;
		char c;

		//----------- 
		// Declaration de vairables locales à la méthode main():
		// Types de base qui existent en C
		//----------- 
		byte by;
		boolean b;
		
		//----------- 
		// Affectation de qques valeurs
		//----------- 
		
	}

}
