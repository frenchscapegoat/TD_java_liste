
public class Assurance {

	public static void main(String[] args) {
		Voiture scenic;
		scenic = new Voiture();
		
		// Affecter une valeur à l'attribut de l'objet scenic
		scenic.annee = 1998;
		
		// Afficher valeur
		System.out.println("Année du Scénic " +scenic);
		
		// Declaarer reference vers une 2e voiture
		Voiture audi = new Voiture();
		
		
		// Créer un objet, modifier lesattributs et afficher
	}

}
